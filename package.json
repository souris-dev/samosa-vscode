{
    "name": "slang-vscode",
    "displayName": "slang-vscode",
    "description": "A simple VS Code extension for slang.",
    "version": "0.0.1",
    "icon": "images/logo_384px.png",
    "repository": {
        "url": "https://github.com/souris-dev/slang-vscode-ext"
    },
    "publisher": "SourisAsh",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "samosa",
            "aliases": ["Samosa", "samosa"],
            "extensions": [".samo",".smo"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "samosa",
            "scopeName": "source.samosa",
            "path": "./syntaxes/samosa.tmLanguage.json"
        }]
    }
}